{
    "dataset_reader": {
        "type": null
    },
    "train_data_path": null,
    "validation_data_path": null,
    "model": {
        "type": "classifier_with_metrics",
        "metrics": {
            "categorical_accuracy": {},
            "f1": {
                "positive_label": 1
            }
        },
        "loss_weights": [
            0.1,
            1.25
        ],
        "text_field_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "embedding",
                    "embedding_dim": 300,          
                    "pretrained_file": "https://allennlp.s3.amazonaws.com/datasets/glove/glove.840B.300d.txt.gz",
                    "trainable": true
                    
                }
            }
        },
        "seq2vec_encoder": {
            "type": "cnn",
            "embedding_dim": 300,
            "num_filters": 300,
            "ngram_filter_sizes": [
                1,
                2,
                3,
                4
            ]
        }
    },
    "iterator": {
        "type": "bucket",
        "sorting_keys": [
            [
                "tokens",
                "num_tokens"
            ]
        ],
        "padding_noise": 0,
        "batch_size": 2
    },
    "trainer": {
        "num_epochs": 15,
        "cuda_device": null,
        "grad_clipping": 1,
        "validation_metric": "+categorical_accuracy",
        "optimizer": {
            "type": "adam"
        }
    }
}

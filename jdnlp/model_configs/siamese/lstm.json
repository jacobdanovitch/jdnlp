{
    "model": {
        "type": "siamese",
        "text_field_embedder": {
            "token_embedders": {
                "tokens": {
                    "type": "embedding",
                    "embedding_dim": 25,
                    "trainable": false
                }
            }
        },
        "left_encoder": {
            "type": "lstm",
            "input_size": 25,
            "hidden_size": 25,
            "num_layers": 1
        }
    },
    "iterator": {
        "type": "basic",
        "batch_size" : 2
    },
    "trainer": {
        "num_epochs": 1,
        "cuda_device": -1,
        "grad_clipping": null,
        "validation_metric": "+loss",
        "optimizer": {
            "type": "adam"
        }
    }
}
{
    "data": {
        "path": "datasets/twtc/train.json",
        "lines": true
    },
    "type": "data_augmenter",
    "transform": {
        "processes": 32,
        "text_col": "text",
        "augments": {
            "prob": null,
            "substitute_word_synonyms": {
                "num": 4,
                "pos": "NOUN"
            },
            "delete_words": {
                "num": 4,
                "pos": "ADJ"
            }
        }
    },
    "write_config": [
        {
            "path": "datasets/twtc/train.json",
            "orient": "records",
            "lines": true
        }
    ]

}